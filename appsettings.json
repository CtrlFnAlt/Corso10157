{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Data Source=Data/Data.db"
  },
  "Courses": {
    "PerPage": 10,
    "Orderd": {
      "By": "Rating",
      "Ascending": false,
      "Allow": [
        "NomeCorso",
        "Rating",
        "Prezzo"
      ]
    }
  },
  "MemoryCache": {
    "SizeLimit": 500
  },
  "ResponseCache": {
    "Home": {
      "Duration": 240,
      "Location": "Any",
      "VaryByQueryKeys": [
        "page"
      ]
    }
  }
}